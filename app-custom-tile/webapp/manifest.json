{
	"_version": "1.12.0",
	"sap.flp": {
		"type": "tile",
		"tileSize": "1x1"
	},
	"sap.app": {
		"id": "project.namespace.customTile",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "Custom Dynamic App Launcher",
		"description": "Custom Tile",
		"tags": {
			"keywords": []
		},
		"ach": "CA-UI2-INT-FE",
		"dataSources": {
			"mainService" : {
				"uri": "notifservice",
				"type" : "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"crossNavigation": {
			"inbounds": {
				"object1-action1": {
					"semanticObject": "object1",
					"action": "action1",
					"signature": {
						"additionalParameters": "allowed"
					}
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"icons": {
			"icon": "sap-icon://favorite"
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"componentName": "project.namespace.customTile",
		"dependencies": {
			"minUI5Version": "1.71.4",
			"libs": {
				"sap.m": {},
				"sap.suite.ui.microchart": {}
			}
		},
		"models": {},

		"rootView": {
			"viewName": "project.namespace.customTile.DynamicTile",
			"type": "XML",
			"async": true,
			"id": "idAppControl"
		},
		"handleValidation": false
	}
}